<body>
 <div class="sidebar navbar-fixed-top">
      <div class="logo">
        <img class="mimg" src="/public/img/Untitled-5.png" alt="">
      </div><hr>
      <div class="navigation">
         <ul class=" navbar-nav ">
         {{#if user}}
           <li><i class="fas menuic fa-home"></i><a href="">Dashboard</a></li>
           <li><i class="fas menuic fa-user-circle"></i><a href="">User Profile</a></li>
           <li><i class="fas menuic fa-chart-bar"></i><a href="">Analitics</a></li>
           <li><i class="fas menuic fa-bell"></i><a href="">Notification</a></li>
          {{else}}
           <li><i class="fas menuic fa-tasks"></i><a href="">Tests</a></li>
          {{/if}}
         </ul>
 
       </div>

    </div>

    
      <div class="top-header col-md-10">
        
        <div class="notifications">
